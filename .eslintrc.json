{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "google"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-unused-vars": 2,
        "max-len": [1, 100],
        "max-params": [2, 3],
        "require-jsdoc" : 0,
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"]
    }
} 